<h1>Josh X San Francisco</h1>

<%= render "new_post"%>

<ul>
  <% @all_posts.each do |post| %>
    <li>
      <%= post.title %>
      <%= post.content %>
      <%= button_to "Delete", { action: "destroy", id: post.id }, method: :delete, data: { confirm: "Are you sure?" } %><br>
    </li>
  <% end %>
</ul>