<h1>Josh X San Francisco</h1>

<ul>
  <% @posts.each do |post| %>
    <li>
      <%= post.title %><br>
      <%= post.content %>
      <%= button_to "Delete", { action: "destroy", id: post.id }, method: :delete, data: { confirm: "Are you sure?" } %>
      <%= button_to "Edit", { action: "edit", id: post.id }, method: :get %><br>
    </li>
  <% end %>
</ul>